      <HBox layoutX="80.0" layoutY="140.0" spacing="20.0" />
      <Button fx:id="btConfirm" layoutX="317.0" layoutY="103.0" mnemonicParsing="false" onAction="#onConfir" style="-fx-background-color: #C6490B;" text="Confirmar" textFill="WHITE" />
      <Button fx:id="btCancel" layoutX="244.0" layoutY="103.0" mnemonicParsing="false" onAction="#onCanc" style="-fx-border-color: #C6490B; -fx-background-color: #FFFFFF;" text="Cancelar" />
